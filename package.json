{
  "name": "@faharmony/cli",
  "version": "0.0.1",
  "description": "Node command line to install and manage harmony framework.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/faharmony/cli"
  },
  "author": "Siddhant Gupta <siddhant@fasolutions.com>",
  "contributors": [
    "Siddhant Gupta (@guptasiddhant)"
  ],
  "dependencies": {
    "chalk": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "typescript": "^3.9.6",
    "microbundle-crl": "^0.13.11"
  },
  "scripts": {
    "clear": "rm -R ./bin || true",
    "bundle": "yarn clear && microbundle-crl --compress -f cjs --no-sourcemap",
